    <import resource="classpath:ks-base-dictionary.xml"/>

    <bean id="CourseDetails" parent="CourseDetails-parentBean"/>
    <bean id="CourseDetails-parentBean" abstract="true" parent="DataObjectEntry">
        <property name="objectLabel" value="Course Details"/>
        <property name="dataObjectClass" value="org.kuali.student.myplan.course.dataobject.CourseDetails"/>
        <property name="titleAttribute" value="courseId"/>
        <property name="attributes">
            <list>
                <ref bean="CourseDetails.courseId"/>
                <ref bean="CourseDetails.code"/>
                <ref bean="CourseDetails.courseTitle"/>
                <ref bean="CourseDetails.credit"/>
                <ref bean="CourseDetails.courseDescription"/>
                <ref bean="CourseDetails.curriculumTitle"/>
                <ref bean="CourseDetails.lastOffered"/>
            </list>
        </property>

        <property name="collections">
            <list>
                <ref bean="CourseDetails.campusLocations"/>
                <ref bean="CourseDetails.scheduledTerms"/>
                <ref bean="CourseDetails.termsOffered"/>
                <ref bean="CourseDetails.requisites"/>
                <ref bean="CourseDetails.genEdRequirements"/>

            </list>
        </property>
    </bean>

    <bean id="CourseDetails.courseId" parent="CourseDetails.courseId-parentBean"/>
    <bean id="CourseDetails.courseId-parentBean" abstract="true" parent="BaseKuali.idField">
        <property name="name" value="courseId"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Course ID"/>
        <property name="shortLabel" value="ID"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="100"/>
        <property name="validCharactersConstraint">
            <bean parent="NoWhitespacePatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="100"/>
        </property>
    </bean>

    <bean id="CourseDetails.code" parent="CourseDetails.code-parentBean"/>
    <bean id="CourseDetails.code-parentBean" abstract="true" parent="BaseKuali.string">
        <property name="name" value="code"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Course Code"/>
        <property name="shortLabel" value="Course"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="10"/>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="10"/>
        </property>
    </bean>

    <bean id="CourseDetails.courseTitle" parent="CourseDetails.courseTitle-parentBean"/>
    <bean id="CourseDetails.courseTitle-parentBean" abstract="true" parent="BaseKuali.string">
        <property name="name" value="courseTitle"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Course Title"/>
        <property name="shortLabel" value="Course Title"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="40"/>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="40"/>
        </property>
    </bean>

    <bean id="CourseDetails.credit" parent="CourseDetails.credit-parentBean"/>
    <bean id="CourseDetails.credit-parentBean" abstract="true" parent="BaseKuali.string">
        <property name="name" value="credit"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Course Credit"/>
        <property name="shortLabel" value="Credit"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="20"/>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="20"/>
        </property>
    </bean>

        <bean id="CourseDetails.courseDescription" parent="CourseDetails.courseDescription-parentBean"/>
    <bean id="CourseDetails.courseDescription-parentBean" abstract="true" parent="BaseKuali.stringLong">
        <property name="name" value="courseDescription"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Course Description"/>
        <property name="shortLabel" value="Description"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="200"/>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="200"/>
        </property>
    </bean>

    <bean id="CourseDetails.campusLocations" parent="CourseDetails.campusLocations-parentBean"/>
    <bean id="CourseDetails.campusLocations-parentBean" abstract="true" parent="CollectionDefinition">
        <property name="name" value="campusLocations"/>
        <property name="shortLabel" value="Locations"/>
        <property name="summary">
            <value>Campus Locations</value>
        </property>
        <property name="label" value="Campus Locations"/>
        <property name="elementLabel" value="Campus Locations"/>
        <property name="minOccurs" value="0"/>
        <property name="dataObjectClass" value="java.lang.String"/>
    </bean>

    <bean id="CourseDetails.scheduledTerms" parent="CourseDetails.scheduledTerms-parentBean"/>
    <bean id="CourseDetails.scheduledTerms-parentBean" abstract="true" parent="CollectionDefinition">
        <property name="name" value="scheduledTerms"/>
        <property name="shortLabel" value="Scheduled Quarter"/>
        <property name="summary">
            <value>Scheduled Quarter</value>
        </property>
        <property name="label" value="Scheduled Quarter"/>
        <property name="minOccurs" value="0"/>
        <property name="dataObjectClass" value="java.lang.String"/>
    </bean>

    <bean id="CourseDetails.termsOffered" parent="CourseDetails.termsOffered-parentBean"/>
    <bean id="CourseDetails.termsOffered-parentBean" abstract="true" parent="CollectionDefinition">
        <property name="name" value="termsOffered"/>
        <property name="shortLabel" value="Projected Quarter"/>
        <property name="summary">
            <value>Projected Quarter</value>
        </property>
        <property name="label" value="Projected Quarter"/>
        <property name="minOccurs" value="0"/>
        <property name="dataObjectClass" value="java.lang.String"/>
    </bean>

    <bean id="CourseDetails.lastOffered" parent="CourseDetails.lastOffered-parentBean"/>
    <bean id="CourseDetails.lastOffered-parentBean" abstract="true" parent="BaseKuali.stringLong">
        <property name="name" value="lastOffered"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Last Offered"/>
        <property name="shortLabel" value="Last Offered"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="200"/>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="200"/>
        </property>
    </bean>

    <bean id="CourseDetails.requisites" parent="CourseDetails.requisites-parentBean"/>
    <bean id="CourseDetails.requisites-parentBean" abstract="true" parent="CollectionDefinition">
        <property name="name" value="requisites"/>
        <property name="shortLabel" value="Course Requisites"/>
        <property name="summary">
            <value>Course Requisites</value>
        </property>
        <property name="label" value="Course Requisites"/>
        <property name="elementLabel" value="Course Requisites"/>
        <property name="minOccurs" value="0"/>
        <property name="dataObjectClass" value="java.lang.String"/>
    </bean>

    <bean id="CourseDetails.genEdRequirements" parent="CourseDetails.genEdRequirements-parentBean"/>
    <bean id="CourseDetails.genEdRequirements-parentBean" abstract="true" parent="CollectionDefinition">
        <property name="name" value="genEdRequirements"/>
        <property name="shortLabel" value="Gen Edu. Requirements"/>
        <property name="summary">
            <value>General Educational Requirements</value>
        </property>
        <property name="label" value="Gen Edu. Requirements"/>
        <property name="elementLabel" value="Gen Edu. Requirements"/>
        <property name="minOccurs" value="0"/>
        <property name="dataObjectClass" value="java.lang.String"/>
    </bean>

    <bean id="CourseDetails.curriculumTitle" parent="CourseDetails.curriculumTitle-parentBean"/>
    <bean id="CourseDetails.curriculumTitle-parentBean" abstract="true" parent="BaseKuali.string">
        <property name="name" value="curriculumTitle"/>
        <property name="dataType" value="STRING"/>
        <property name="label" value="Curriculum"/>
        <property name="shortLabel" value="Curriculum"/>
        <property name="forceUppercase" value="false"/>
        <property name="maxLength" value="200"/>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="200"/>
        </property>
    </bean>


    <bean id="CourseDetails-InquiryView" parent="MyPlanInquiryView">
        <property name="dataObjectClassName" value="org.kuali.student.myplan.course.dataobject.CourseDetails"/>
        <property name="viewHelperServiceClassName" value="org.kuali.student.myplan.course.service.CourseDetailsInquiryViewHelperServiceImpl"/>
        <property name="breadcrumbs.render" value="false"/>
        <property name="page.header.render" value="false"/>
        <property name="footer.render" value="false"/>
        <property name="singlePageView" value="true"/>
        <property name="styleClasses">
            <list merge="true">
                <value>myplan-view</value>
            </list>
        </property>
        <property name="header">
            <ref bean="course_details_back_link"/>
        </property>
        <property name="Items">
            <list>
                <bean parent="Uif-Page" p:id="course_details_page"
                      p:title="@{dataObject.code} @{dataObject.courseTitle} (@{dataObject.credit})">
                    <property name="layoutManager">
                        <bean parent="Uif-HorizontalBoxLayout"/>
                    </property>
                    <property name="items">
                        <list>
                            <ref bean="course_details_section" />
                            <ref bean="myplan_section" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="course_details_back_link" parent="Uif-ViewHeaderField">
        <property name="items">
            <bean parent="Uif-Link" p:linkLabel="Back to Previous Page" p:title="Back to Previous Page" p:hrefText="javascript:history.go(-1)" p:style="display:block; margin-top:1em;"/>
        </property>
    </bean>

    <bean id="course_details_section" parent="Uif-VerticalBoxSubSection" p:errorsField.render="false">
        <property name="styleClasses">
            <list merge="true">
                <value>myplan-container-720</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-VerticalBoxGroup" p:errorsField.render="false">
                    <property name="items">
                        <list>
                            <ref bean="course_details_actions"/>
                            <ref bean="course_details_overview"/>
                            <ref bean="course_details_links"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
    <bean id="course_details_actions" parent="Uif-HorizontalBoxGroup" p:errorsField.render="false" p:style="margin-bottom: 25px;">
        <property name="items">
            <list>
                <bean parent="Uif-ActionFieldBase" p:actionLabel="Add to Plan"
                      p:clientSideJs="alert('This course has been added successfully');">
                    <property name="styleClasses">
                        <list merge="true">
                            <value>myplan-button</value>
                            <value>myplan-button-blue</value>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-ActionFieldBase" p:actionLabel="Save to Your Courses List" p:id="@{dataObject.courseId}" p:render="@{dataObject.inSavedCourseList ? false : true}">
                    <property name="clientSideJs" value="addSavedCourse('@{dataObject.courseId}', 'addSavedCourse', 'plan', {viewId:'SavedCoursesListActions-FormView', courseId:'@{dataObject.courseId}'}, e);"/>
                    <property name="styleClasses">
                        <list merge="true">
                            <value>myplan-button</value>
                            <value>myplan-button-gray</value>
                        </list>
                    </property>
                </bean>
                <bean parent="Uif-LinkGroup" p:errorsField.render="false" p:id="@{dataObject.courseId}" p:render="@{dataObject.inSavedCourseList ? true : false}"
                      p:groupBeginDelimiter="This course is in " p:groupEndDelimiter="" p:linkSeparator="">
                    <property name="styleClasses">
                        <list merge="true">
                            <value>myplan-message-border</value>
                            <value>myplan-message-info</value>
                        </list>
                    </property>
                    <property name="items">
                    <list>
                      <bean parent="Uif-Link" p:linkLabel="Your Courses List" p:hrefText="lookup?methodToCall=search&amp;viewId=SavedCoursesDetail-LookupView"
                            p:title="Saved Courses List" p:target="" p:labelField.render="false"/>
                    </list>
                  </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="course_details_overview" parent="Uif-GridGroup" p:errorsField.render="false">
        <property name="styleClasses">
            <list merge="true">
                <value>myplan-course-details</value>
            </list>
        </property>
        <property name="layoutManager.numberOfColumns" value="2"/>
        <property name="layoutManager.applyDefaultCellWidths" value="false"/>
        <property name="items">
            <list>
                <!-- Course Description -->
                <bean parent="Uif-DataField" p:label="Course Description" p:propertyName="courseDescription" p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                    <property name="render" value="@{dataObject.courseDescription.bytes.length gt 0 ? true : false}"/>
                </bean>
                <!-- If Course Description is empty-->
                <bean parent="Uif-MessageField" p:label="Course Description" p:messageText="No description is available">
                    <property name="labelField">
                        <bean parent="Uif-Label" p:renderColon="false">
                            <property name="styleClasses">
                                <list merge="true">
                                    <value>myplan-text-align-left</value>
                                    <value>myplan-text-nowrap</value>
                                </list>
                            </property>
                        </bean>
                    </property>
                    <property name="render" value="@{dataObject.courseDescription.bytes.length gt 0 ? false : true}"/>
                </bean>

                <!-- Requisites -->
                <bean parent="Uif-HorizontalFieldGroup" p:label="Course Requisites" p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="requisites" p:labelField.render="false" p:escapeHtmlInPropertyValue="false">
                                <property name="propertyEditor" ref="data_list_formatter"/>
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>myplan-data-list</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField" p:messageText="(Source: Course Catalog)">
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>fl-font-size-90</value>
                                        <value>fl-text-gray</value>
                                    </list>
                                </property>
                            </bean>
                            <!--<bean parent="Uif-LinkGroup" p:errorsField.render="false"
                                  p:groupBeginDelimiter="(Source:" p:groupEndDelimiter=")" p:linkSeparator="">
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>fl-font-size-90</value>
                                        <value>fl-text-gray</value>
                                    </list>
                                </property>
                                <property name="items">
                                <list>
                                  <bean parent="Uif-Link" p:linkLabel="Course Catalog" p:hrefText="http://www.washington.edu/students/crscat/"
                                        p:title="Course Catalog" p:target="" p:labelField.render="false"/>
                                </list>
                              </property>
                            </bean>-->
                        </list>
                    </property>
                </bean>

                <!-- Quarters -->
                <bean parent="Uif-HorizontalFieldGroup" p:label="Quarters" p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="scheduledTerms" p:labelField.render="false" p:escapeHtmlInPropertyValue="false">
                                <property name="propertyEditor" ref="scheduled_terms_list_formatter"/>
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>myplan-data-list</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField" p:messageText="(Source: Time Schedule)">
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>fl-font-size-90</value>
                                        <value>fl-text-gray</value>
                                    </list>
                                </property>
                            </bean>
                            <!--<bean parent="Uif-LinkGroup" p:errorsField.render="false"
                                  p:groupBeginDelimiter="(Source:" p:groupEndDelimiter=")" p:linkSeparator="">
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>fl-font-size-90</value>
                                        <value>fl-text-gray</value>
                                    </list>
                                </property>
                                <property name="items">
                                <list>
                                    &lt;!&ndash; Seattle Time Schedule &ndash;&gt;
                                    <bean parent="Uif-Link" p:linkLabel="Time Schedule" p:title="Time Schedule"
                                              p:hrefText="http://www.washington.edu/students/timeschd/"
                                              p:target="" p:labelField.render="false" p:render="@{dataObject.campusLocations.contains('Seattle') ? true : false }"/>
                                    &lt;!&ndash; Tacoma Time Schedule &ndash;&gt;
                                    <bean parent="Uif-Link" p:linkLabel="Time Schedule" p:title="Time Schedule"
                                              p:hrefText="http://www.washington.edu/students/timeschd/T/"
                                              p:target="" p:labelField.render="false" p:render="@{dataObject.campusLocations.contains('Tacoma') ? true : false }"/>
                                    &lt;!&ndash; Bothell Time Schedule &ndash;&gt;
                                    <bean parent="Uif-Link" p:linkLabel="Time Schedule" p:title="Time Schedule"
                                              p:hrefText="http://www.uwb.edu/registration/time"
                                              p:target="" p:labelField.render="false" p:render="@{dataObject.campusLocations.contains('Bothell') ? true : false }"/>
                                </list>
                              </property>
                            </bean>-->
                        </list>
                    </property>

                </bean>

                <!--Last Offered -->
                <bean parent="Uif-HorizontalFieldGroup" p:label="" p:render="@{dataObject.scheduledTerms.size() gt 0 ? false : true}">
                    <property name="items">
                        <list>
                            <bean parent="Uif-DataField" p:propertyName="lastOffered" p:label="Last offered in " p:render="@{dataObject.lastOffered ne null ? true : false}"
                                  p:labelField.renderColon="false"  p:labelField.styleClasses="myplan-text-normal"/>
                            <bean parent="Uif-MessageField" p:messageText="Not offered for more than 10 years." p:render="@{dataObject.lastOffered eq null ? true : false}"/>
                        </list>
                    </property>
                </bean>

                <!-- Terms -->
                <bean parent="Uif-HorizontalFieldGroup" p:label="">
                    <property name="items">
                        <list>

                            <bean parent="Uif-MessageField" p:messageText="Projected to be offered in " p:render="@{dataObject.termsOffered.size() gt 0 ? true : false}"/>
                            <bean parent="Uif-DataField" p:propertyName="termsOffered" p:labelField.render="false">
                                <property name="propertyEditor" ref="terms_formatter"/>
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>myplan-data-list</value>
                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-MessageField" p:messageText="quarters" p:render="@{dataObject.termsOffered.size() gt 0 ? true : false}"/>
                            <bean parent="Uif-MessageField" p:messageText="(Source: Course Catalog)">
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>fl-font-size-90</value>
                                        <value>fl-text-gray</value>
                                    </list>
                                </property>
                            </bean>
                            <!--<bean parent="Uif-LinkGroup" p:errorsField.render="false"
                                  p:groupBeginDelimiter="(Source:" p:groupEndDelimiter=")" p:linkSeparator="">
                                <property name="styleClasses">
                                    <list merge="true">
                                        <value>fl-font-size-90</value>
                                        <value>fl-text-gray</value>
                                    </list>
                                </property>
                                <property name="items">
                                <list>
                                  <bean parent="Uif-Link" p:linkLabel="Course Catalog" p:hrefText="http://www.washington.edu/students/crscat/"
                                        p:title="Course Catalog" p:target="" p:labelField.render="false"/>
                                </list>
                              </property>
                            </bean>-->
                        </list>
                    </property>
                </bean>
                <!-- Credits -->
                <bean parent="Uif-DataField" p:propertyName="credit" p:label="Credits" p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                </bean>

                <!-- Gen Ed Requirements -->
                <bean parent="Uif-DataField" p:propertyName="genEdRequirements" p:label="General Edu. Req." p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                    <property name="styleClasses">
                        <list merge="true">
                            <value>myplan-stacked-list</value>
                        </list>
                    </property>
                    <property name="propertyEditor" ref="data_list_formatter"/>
                </bean>

                <!--Curriculum -->
                <bean parent="Uif-DataField" p:propertyName="curriculumTitle" p:label="Curriculum" p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                </bean>

                <!-- Campus Locations -->
                <bean parent="Uif-DataField" p:propertyName="campusLocations" p:label="Campus" p:labelField.renderColon="false">
                    <property name="labelField.styleClasses">
                        <list merge="true">
                            <value>myplan-text-align-left</value>
                            <value>myplan-text-nowrap</value>
                        </list>
                    </property>
                    <property name="styleClasses">
                        <list merge="true">
                            <value>myplan-data-list</value>
                        </list>
                    </property>
                    <property name="propertyEditor" ref="data_list_formatter"/>
                </bean>
            </list>
        </property>
    </bean>

