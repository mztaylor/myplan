    <bean id="course_search_results_list_items" parent="CollectionGroupTableLayout"
          p:layoutManager.numberOfColumns="12">
        <property name="disclosure.render" value="false"/>
        <property name="collectionObjectClass" value="org.kuali.student.myplan.course.dataobject.CourseSearchItem"/>
        <property name="propertyName" value="courseSearchResults"/>
        <property name="renderAddLine" value="false"/>
        <property name="renderLineActions" value="false"/>
        <property name="layoutManager.id" value="course_search_results_datatable"/>
        <property name="layoutManager.sequencePropertyName" value="code"/>
        <property name="layoutManager.sequenceFieldPrototype.label" value="Code"/>
        <property name="layoutManager.applyDefaultCellWidths" value="false"/>
        <property name="layoutManager.renderSequenceField" value="false"/>
        <property name="bindingInfo.bindToForm" value="true"/>
        <!-- Have to turn off emptyTableMessage, so that oLanguage option is not overridden by code -->
        <property name="layoutManager.richTable.emptyTableMessage" value=""/>
        <!-- Have to enable disableTableSort, so that aoColumns option is not overridden by code -->
        <property name="layoutManager.richTable.disableTableSort"
                  value="true"/>
        <property name="layoutManager.richTable.componentOptions">
            <map merge="true">

                <entry key="oLanguage"
                       value="{'sLengthMenu':'Show _MENU_', 'sEmptyTable':'0 results found', 'sZeroRecords':'0 results found', 'sProcessing':'Loading...', 'sInfo':'Showing _START_-_END_ of _TOTAL_ results', 'sInfoFiltered':''}"/>
                <entry key="iDisplayLength" value="20"/>
                <entry key="aLengthMenu" value="[ 20, 50, 100 ]"/>
                <entry key="sDom" value="ilrtSp"/>
                <entry key="sPaginationType" value="full_numbers"/>
                <entry key="bRetrieve" value="true"/>
                <entry key="bAutoWidth" value="false"/>
                <entry key="bDeferRender" value="true"/>
                <entry key="bScrollCollapse" value="true"/>
                <entry key="aoColumns"
                       value="[
                            {'bSortable':true, 'bSearchable':false},
                            {'bSortable':true, 'bSearchable':false, 'sWidth':'175px'},
                            {'bSortable':false, 'bSearchable':false},
                            {'bSortable':false, 'bSearchable':false},
                            {'bSortable':false, 'bSearchable':false},
                            {'bSortable':false, 'bSearchable':false},
                            {'sTitle':'facetQuarter', 'bVisible':false},
                            {'sTitle':'facetScheduled', 'bVisible':false},
                            {'sTitle':'facetGenEduReq', 'bVisible':false},
                            {'sTitle':'facetCredits', 'bVisible':false},
                            {'sTitle':'facetLevel', 'bVisible':false},
                            {'sTitle':'facetCurriculum', 'bVisible':false}
                        ]"/>
                <!--<entry key="fnDrawCallback"
                       value="function() {
                            if (Math.ceil((this.fnSettings().fnRecordsDisplay()) / this.fnSettings()._iDisplayLength) > 1)  {
                                $('.dataTables_paginate').show();
                            } else {
                                $('.dataTables_paginate').hide();
                            }
                       }"/>-->
            </map>
        </property>
        <property name="items">
            <list>
                <bean parent="DataField" p:propertyName="code" p:label="Code" p:readOnly="true">
                    <property name="styleClasses" value="courseCode"/>
                </bean>

                <!--  TODO: This is a better way to create this field (once InputField is available).
                <bean parent="InputField" p:propertyName="code" p:label="Code" p:styleClasses="courseCode">
                    <property name="fieldInquiry.dataObjectClassName" value="org.kuali.student.lum.course.dto.CourseInfo"/>
                    <property name="fieldInquiry.inquiryParameters">
                        <map>
                            <entry key="courseCode" value="code"/>
                        </map>
                    </property>
                    </bean>
                -->

                <bean parent="LinkField" p:label="Course Name" p:title="@{#line.courseName}" p:linkLabel="@{#line.courseName}" p:styleClasses="courseName" p:target=""
                      p:hrefText="/myplan-embedded-dev/myplan/inquiry?methodToCall=start&amp;dataObjectClassName=org.kuali.student.myplan.course.dataobject.CourseDetails&amp;courseId=@{#line.courseId}" />

                <!--
                <bean parent="InputField" p:propertyName="courseName"
                      p:label="Course Name" p:readOnly="true">
                    <property name="styleClasses" value="courseName"/>
                    <property name="control">
                        <bean parent="TextControl"/>
                    </property>
                </bean>  -->
                <bean parent="DataField" p:propertyName="credit" p:label="Credits"
                      p:readOnly="true">
                    <property name="styleClasses" value="courseCredits"/>
                </bean>
                <bean parent="DataField" p:propertyName="scheduledAndOfferedTerms"
                      p:label="Quarter Offered" p:readOnly="true">
                    <property name="styleClasses" value="courseScheduledTime"/>
                </bean>
                <bean parent="DataField" p:propertyName="genEduReq"
                      p:label="Gen Edu Req" p:readOnly="true">
                    <property name="styleClasses" value="courseGenEduReq"/>
                </bean>
                <bean parent="ActionImageField" p:label="Add" p:clientSideJs="alert('Selected course has been added successfully');">
                    <property name="actionImage">
                        <bean parent="ImageField" p:source="/myplan-embedded-dev/ks-myplan/images/btnAdd.png" p:altText="Add course to MyPlan"/>
                    </property>
                </bean>
                <bean parent="DataField" p:propertyName="termsFacetKey" p:label="Quarter Projected"
                      p:readOnly="true">
                </bean>
                <bean parent="DataField" p:propertyName="scheduledFacetKey" p:label="Quarter Scheduled"
                      p:readOnly="true">
                </bean>
                <bean parent="DataField" p:propertyName="genEduReqFacetKey" p:label="Gen Edu Req"
                      p:readOnly="true">
                </bean>
                <bean parent="DataField" p:propertyName="creditsFacetKey" p:label="Credits"
                      p:readOnly="true">
                </bean>
                <bean parent="DataField" p:propertyName="courseLevelFacetKey" p:label="Course Level"
                      p:readOnly="true">
                </bean>
                <bean parent="DataField" p:propertyName="curriculumFacetKey" p:label="Course Prefix"
                      p:readOnly="true">
                </bean>
            </list>
        </property>
    </bean>


