    <bean id="courseSearchView" parent="KSFormView">
        <property name="id" value="courseSearchView"/>
        <property name="breadcrumbs.render" value="false"/>
        <property name="styleClasses" value="myplan"/>
        <property name="entryPageId" value="course_search"/>

        <property name="additionalCssFiles">
            <list>
                <value>/ks-myplan/css/course-search.css</value>
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list>
                <value>/ks-myplan/scripts/course-search.js</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Page" p:id="course_search">
                    <property name="title" value="Find Courses"/>
                    <property name="items">
                        <list>
                            <ref bean="course_search_page"/>
                        </list>
                    </property>
                    <property name="footer">
                        <bean parent="Footer"/>
                    </property>
                </bean>
                <bean parent="Page" p:id="course_search_result">
                    <property name="title" value="Find Courses"/>
                    <property name="items">
                        <list>
                            <ref bean="course_search_result_page"/>
                        </list>
                    </property>
                    <property name="footer">
                        <bean parent="Footer"/>
                    </property>
                </bean>
                <bean parent="Page" p:id="course_search_no_results">
                    <property name="title" value="Find Courses"/>
                    <property name="items">
                        <list>
                            <ref bean="course_search_no_results_page"/>
                        </list>
                    </property>
                    <property name="footer">
                        <bean parent="Footer"/>
                    </property>
                </bean>
            </list>
        </property>
        <property name="footer.render" value="false"/>
        <property name="formClass"
                  value="org.kuali.student.myplan.course.form.CourseSearchForm"/>
    </bean>


    <!-- Layout for Course Search Form Page -->
    <bean id="course_search_page" parent="Group" p:layoutManager.orientation="HORIZONTAL">
        <!--<property name="layoutManager">
            <bean parent="BoxLayoutManager"/>
        </property>-->
        <property name="disclosure.render" value="false"/>
        <property name="styleClasses" value="fl-container-auto"/>
        <property name="items">
            <list>
                <ref bean="course_search_panel"/>
                <ref bean="mystuff_panel"/>
            </list>
        </property>
    </bean>


