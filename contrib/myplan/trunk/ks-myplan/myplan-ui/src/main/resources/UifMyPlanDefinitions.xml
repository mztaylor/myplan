    <bean id="KSAP-HelpImage" parent="Uif-HelpImage"
          p:source="@{#ConfigProperties['ks.myplan.externalizable.images.url']}/icon_help.png" p:altText="" p:title="" p:cssClasses="helpImage">
    </bean>

    <bean id="KSAP-Tooltip" parent="Uif-Tooltip">
        <property name="onFocus" value="true"/>
        <property name="onMouseHover" value="true"/>
        <property name="templateOptions">
            <map merge="true">
                <entry key="position" value="top"/>
                <entry key="align" value="center"/>
                <entry key="tail" value="{align:'center', hidden:false}"/>
                <entry key="openingSpeed" value="50"/>
                <entry key="closingSpeed" value="50"/>
                <entry key="openingDelay" value="500"/>
                <entry key="closingDelay" value="0"/>
                <entry key="themeMargins" value="{total:'18px', difference:'8px'}"/>
                <entry key="themePath" value="../themes/ksap/images/popover-theme/"/>
                <entry key="themeName" value="help"/>
                <entry key="distance" value="0px"/>
            </map>
        </property>
    </bean>

    <bean id="KSAP-TooltipRight" parent="KSAP-Tooltip">
        <property name="templateOptions">
            <map merge="true">
                <entry key="align" value="right"/>
                <entry key="tail" value="{ align:'right', hidden: false }"/>
            </map>
        </property>
    </bean>

    <bean id="KSAP-TooltipLeft" parent="KSAP-Tooltip">
        <property name="templateOptions">
            <map merge="true">
                <entry key="align" value="left"/>
                <entry key="tail" value="{ align:'left', hidden: false }"/>
                <entry key="themeMargins" value="{ total:'17px', difference:'8px' }"/>
            </map>
        </property>
    </bean>

    <bean id="MyPlanRegistrationStatusIcon" parent="Uif-Image"
