    <bean id="KSAP-GlobalApplicationHeader" parent="Uif-GlobalApplicationHeader" p:templateName="ksap_app_header"
          p:template="/themes/ksap/templates/ksapApplicationHeader.ftl" p:render="true"/>

    <bean id="KSAP-GlobalApplicationFooter" parent="Uif-GlobalApplicationFooter" p:templateName="ksap_app_footer"
          p:template="/themes/ksap/templates/ksapApplicationFooter.ftl" p:render="true"/>

    <bean id="MyPlanFooterDocument" parent="Uif-FooterBase">
        <property name="items">
            <list>
                <bean parent="KSAP-PrimaryActionButton" p:methodToCall="performRouteReport" p:actionLabel="Route Report"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_PERFORM_ROUTE_REPORT]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:methodToCall="sendAdHocRequests" p:actionLabel="Send Adhoc"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SEND_ADHOC_REQUESTS]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:actionScript="validateAndSubmit('route', replacePage);"
                      p:actionLabel="Submit"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:actionScript="validateAndSubmit('save', replacePage);"
                      p:actionLabel="Save"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_SAVE]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:methodToCall="reload" p:actionLabel="Reload"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_RELOAD]}"/>
                <bean parent="KSAP-PrimaryActionButton"
                      p:actionScript="validateAndSubmit('blanketApprove', replacePage);" p:actionLabel="Blanket Approve"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_BLANKET_APPROVE]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:actionScript="validateAndSubmit('approve', replacePage);"
                      p:actionLabel="Approve"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_APPROVE]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:methodToCall="disapprove" p:actionLabel="Disapprove"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_DISAPPROVE]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:methodToCall="fyi" p:actionLabel="FYI"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_FYI]}"/>
                <bean parent="KSAP-PrimaryActionButton" p:methodToCall="acknowledge" p:actionLabel="Acknowledge"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ACKNOWLEDGE]}"/>
                <bean parent="KSAP-SecondaryActionButton" p:methodToCall="close" p:actionLabel="Close"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CLOSE]}"/>
                <bean parent="Uif-ActionLink" p:methodToCall="cancel" p:actionLabel="Cancel"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL]}"/>
                <bean parent="Uif-ActionLink" p:actionScript="validateAndSubmit('copy', replacePage);"
                      p:actionLabel="Copy"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_COPY]}"/>
            </list>
        </property>
    </bean>

    <bean id="MyPlanFooter_SubmitCancel" parent="MyPlanFooterDocument">
        <property name="items">
            <list>
                <bean parent="KSAP-PrimaryActionButton" p:actionScript="validateAndSubmit('route', replacePage);"
                      p:actionLabel="Submit"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_ROUTE]}"/>
                <bean parent="Uif-ActionLink" p:methodToCall="cancel" p:actionLabel="Cancel"
                      p:render="@{#actionFlags[#Constants.KUALI_ACTION_CAN_CANCEL]}"/>
            </list>
        </property>
    </bean>

