    <bean id="MyPlanView" abstract="true" parent="Uif-View">
        <property name="theme">
            <bean parent="MyPlanBaseTheme"/>
        </property>
        <property name="persistFormToSession" value="false"/>
        <property name="breadcrumbs.render" value="false"/>
        <property name="applyDirtyCheck" value="false"/>
        <property name="onLoadScript" value="
                    try{Typekit.load();}catch(e){}
                    setConfigParam('adviser', @{T(org.kuali.student.myplan.plan.util.AtpHelper).getUserSessionHelper().isAdviser()});
                    setConfigParam('ksapImageLocation', '@{#ConfigProperties['ks.myplan.externalizable.images.url']}/');
        "/>
        <property name="header">
            <bean parent="Uif-ViewHeader">
                <property name="headerLevel" value="h1"/>
            </bean>
        </property>
    </bean>

