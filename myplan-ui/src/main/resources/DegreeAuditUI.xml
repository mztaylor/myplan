    <bean id="degreeAuditView" parent="MyPlanFormView">
        <property name="id" value="degreeAuditView"/>
        <property name="breadcrumbs.render" value="false"/>
        <property name="styleClasses" value="myplan"/>
        <property name="entryPageId" value="degree_audit"/>
        <property name="items">
            <list>
                <bean parent="Uif-Page" p:id="degree_audit" p:title="Degree Audit">
                    <property name="items">
                        <list>

                            <bean id="whoops" parent="Uif-MessageField" p:messageText="@{getAuditText()}"/>
                            <bean id="yippee" parent="Uif-MessageField" p:messageText="@{getAuditHtml()}"/>

                        </list>
                    </property>
                </bean>
            </list>
        </property>



        <property name="footer.render" value="false"/>
        <property name="formClass" value="org.kuali.student.myplan.course.form.DegreeAuditForm"/>
    </bean>

</beans>
